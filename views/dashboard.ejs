        <!-- Community members Section -->
        <div class="users">
            <h1>Community Friends</h1>
            <div class="user-list">
                <div class="user">
                    <img src="/images/avatar1.png">
                    <h2>Om</h2>
                </div>
                <div class="user">
                    <img src="/images/avatar2.png">
                    <h2>Amanullah</h2>
                </div>
                <div class="user">
                    <img src="/images/avatar3.png">
                    <h2>Subhamay</h2>
                </div>
                <div class="user">
                    <img src="/images/plus.png">
                    <h2>Find More</h2>
                </div>
            </div>
        </div>
        <!-- End of Community Members Section -->

        <div class="recent-problem">
            <h2>Recent Problems</h2>
            <table>
                <thead>
                    <tr>
                        <th>Problem Name</th>
                        <th>Language</th>
                        <th>Course</th>
                        <th>Status</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Reverse String</td>
                        <td>Python</td>
                        <td>Python Programming</td>
                        <td class="warning">Pending</td>
                        <td class="primary">Details</td>
                    </tr>
                    <tr>
                        <td>Substring</td>
                        <td>Javascript</td>
                        <td>Javascript Programming</td>
                        <td class="danger">Not Done</td>
                        <td class="primary">Details</td>
                    </tr>
                    <tr>
                        <td>Polymorphism</td>
                        <td>C++</td>
                        <td>C++ Course</td>
                        <td class="primary">Done</td>
                        <td class="primary">Details</td>
